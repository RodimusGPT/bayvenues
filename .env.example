# API Keys for BayVenues
# Copy this file to .env and fill in your keys

# Google Maps API key (used by the app for map display)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Google Places API key (for scripts/enrich-places-api.cjs)
GOOGLE_PLACES_API_KEY=your_google_places_api_key

# YouTube Data API key (for scripts/fetch-youtube-videos.cjs)
YOUTUBE_API_KEY=your_youtube_api_key

# Serper API key for image search (for scripts/fetch-google-images.cjs)
# Get your key at https://serper.dev
SERPER_API_KEY=your_serper_api_key
